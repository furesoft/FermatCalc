<UserControl xmlns="https://github.com/avaloniaui"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 xmlns:icons="clr-namespace:Material.Icons.Avalonia;assembly=Material.Icons.Avalonia"
			 xmlns:material="clr-namespace:Material.Styles.Controls;assembly=Material.Styles"
			 xmlns:controls="clr-namespace:FermatCalc.Controls;assembly=FermatCalc.Controls"
			 mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
			 x:Class="FermatCalc.Views.Pages.CalculatorPage">
	<StackPanel Margin="0,25,0,0">
		<controls:Display HorizontalAlignment="Stretch" VerticalAlignment="Top" Margin="35,0,35,0" Height="125" Background="#606745" />

		<Grid Margin="35,15,0,0">
			<controls:NavigationButtonGroup Margin="0,25,0,0" HorizontalAlignment="Center" />
			<Button Content="Shift" Width="60" Height="40" Classes="Outline" HorizontalAlignment="Left" VerticalAlignment="Top" />
		</Grid>
	</StackPanel>
</UserControl>